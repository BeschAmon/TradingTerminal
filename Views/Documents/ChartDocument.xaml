<UserControl x:Class="TradingTerminal.Views.Documents.ChartDocument"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TradingTerminal.Views.Documents"
             mc:Ignorable="d"
             xmlns:vm="clr-namespace:TradingTerminal.ViewModels.Documents"
             d:DataContext="{d:DesignInstance vm:ChartDocument}">
    <Grid>
        <Button Command="{Binding TestCommand}"
                Content="Text"/>
    </Grid>
</UserControl>
